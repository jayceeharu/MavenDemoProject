pipeline {
    agent any
    
    tools {
		maven 'Maven-3.8.4'
	
	}

    stages {
        stage ('Compile Stage') {

            steps {
                
                  bat  'mvn clean compile'
                }
            
        }

        stage ('Testing Stage') {

            steps {
                    bat  'mvn test'
                }
            
        }


        stage ('Deployment Stage') {
            steps {
                echo "deploying"
           }
        }
    }
}
